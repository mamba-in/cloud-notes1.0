    "husky": "1.3.1",
    "lint-staged": "7.2.0",
    "markdown-toc": "1.2.0",
    "markdown-it-include": "^1.0.0",
    "prettier": "^1.14.0",
    "vuepress": "^1.1.0"
  },
  "dependencies": {
    "node-forge": ">=0.10.0",
    "http-proxy": ">=1.18.1",
    "dot-prop": ">=4.2.1",
    "prismjs": ">=1.24.0",
    "elliptic": ">=6.5.3",
    "serialize-javascript": ">=3.1.0",
    "acorn": ">=6.4.1",
